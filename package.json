{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "build:all": "pnpm run clean && pnpm run build && pnpm run thumbnail && pnpm run generate:redirects && pnpm run generate:meta",
    "build": "pnpm -r run build",
